{
  "name": "swagger-chunk",
  "version": "1.0.1",
  "description": "Build and maintain large swagger API documentation files from small manageable chunks. Output yaml or json format.",
  "keywords": [
    "swagger",
    "swagger-codegen",
    "yaml",
    "yml",
    "merge yaml",
    "merge yml",
    "DDD",
    "Documentation-Driven Development"
  ],
  "main": "dist/SwaggerChunk.js",
  "module": "dist/SwaggerChunk.mjs",
  "jsnext:main": "dist/SwaggerChunk.mjs",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "build:dev": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdcrecur/swagger-chunk.git"
  },
  "author": "John Carmichael",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdcrecur/swagger-chunk/issues"
  },
  "homepage": "https://github.com/jdcrecur/swagger-chunk#readme",
  "files": [
    "lib",
    "dist"
  ],
  "dependencies": {
    "commander": "^2.14.1",
    "json-refs": "2.1.7",
    "js-yaml": "3.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "rollup-plugin-babel": "^3.0.3"
  }
}
